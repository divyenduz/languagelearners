    "tunnel": "lt --port 3000 --subdomain languagelearnersclub",
    "build": "tsc",
    "watch": "tsc -w",
    "set-webhook": "ts-node ./scripts/set-webhook.ts",
    "set-webhook-production": "ts-node ./scripts/set-webhook.ts --production",
    "get-bot": "ts-node ./scripts/get-bot.ts",
    "get-bot-production": "ts-node ./scripts/get-bot.ts --production",
    "migrate": "echo 'I removed prisma and introspected this DB, migrations would probably happen in SQL'",
    "migrate-production": "echo 'I removed prisma and introspected this DB, migrations would probably happen in SQL'",
    "deploy": "sls deploy --aws-profile zoid && yarn set-webhook",
    "deploy-production": "NODE_ENV=production sls deploy --stage production --aws-profile zoid"
  }
